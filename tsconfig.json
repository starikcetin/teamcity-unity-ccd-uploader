{
  "compilerOptions": {
    "lib": [
      "es2017",
    ],
    "module": "commonjs",
    "target": "es2017",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "pretty": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "rootDir": "./src",
    "noEmit": false,
    "incremental": false,
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}